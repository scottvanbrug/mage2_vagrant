{
    "name": "ebayenterprise/mage2_vagrant",
    "description": "Meta project for create a dev environment for M2 work",
    "require": {
        "magento/product-community-edition": "~0.0",
        "magento-hackathon/composer-command-integrator": "~1.1"
    },
    "repositories": {
        "composer-command-integrator": {
            "type": "vcs",
            "url": "https://github.com/magento-hackathon/composer-command-integrator.git"
        },
        "magento": {
            "type": "composer",
            "url": "http://packages.magento.com/"
        }
    },
    "extra": {
        "magento-root-dir": ".",
        "magento-deploystrategy": "copy",
        "magento-force": true
    },
    "minimum-stability": "dev"
}
